{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:react/jsx-runtime",
        "airbnb",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "quotes": ["error", "single"],
        "object-shorthand": ["always"],
        "react/react-in-jsx-scope": "off",
        "global-require": "off",
        "no-dynamic-require":"off",
        "react/prop-types": "off",
        "no-unused-vars": "off"
    }
}
